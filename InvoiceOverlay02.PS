%-------------------------------------------------------ISO Logo

/outsidecircletext
 {circtextdict begin
  /radius exch def
  /centerangle exch def
  /ptsize exch def
  /str exch def
  /xradius radius ptsize 4 div add def
  
  gsave
   centerangle str findhalfangle add rotate
   
  str
   {/charcode exch def
    ( ) dup 0 charcode put outsideplacechar
   } forall
  grestore
  end
 } def
 
/insidecircletext
 {circtextdict begin
  /radius exch def
  /centerangle exch def
  /ptsize exch def
  /str exch def
  
  /xradius radius ptsize 3 div sub def
  gsave
   centerangle str findhalfangle sub rotate
   str
    {/charcode exch def
     ( ) dup 0 charcode put insideplacechar
    } forall
   grestore
   end
  } def
   
/circtextdict 16 dict def
circtextdict begin
 /findhalfangle
  {stringwidth pop 2 div
   2 xradius mul pi mul div 360 mul
  } def
  
/outsideplacechar
 {/char exch def
  /halfangle char findhalfangle def
  gsave
   halfangle neg rotate
   radius 0 translate
   -90 rotate
   char stringwidth pop 2 div neg 0 moveto
   char show 
   grestore
   halfangle 2 mul neg rotate
  } def
  
/insideplacechar
 {/char exch def
 /halfangle char findhalfangle def
 gsave
  halfangle rotate
  radius 0 translate
  90 rotate
  char stringwidth pop 2 div neg 0 moveto
  char show
 grestore
 halfangle 2 mul rotate
} def

/pi 3.1415923 def
end

